{
    "name": "dariassoft/laravel-search-string",
    "type": "library",
    "description": "Generates database queries based on one unique string using a simple and customizable syntax. Laravel 11 Compatible",
    "license": "MIT",
    "authors": [
        {
            "name": "Loris Leiva",
            "email": "loris.leiva@gmail.com"
        },
        {
          "name": "Daniel Esteban Arias (Fork Maintainer)",
          "email": "dariassoft@gmail.com",
          "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^11.0|^12.0",
        "hoa/compiler": "^3.17",
        "sanmai/hoa-protocol": "^1.17"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5|^11.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
       "psr-4": {
            "Lorisleiva\\LaravelSearchString\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lorisleiva\\LaravelSearchString\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lorisleiva\\LaravelSearchString\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
